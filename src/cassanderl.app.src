{application, cassanderl, [
    {description, "A simple Cassandra client"},
    {vsn, "0.1"},
    {registered, []},
    {applications, [kernel, stdlib]},
    {mod, {cassanderl_app, []}},
    {env, [{options,
               [{pool_size, 10},
                {max_overflow, 20},
                {hostname, "127.0.0.1"},
                {port, 9160},
                {keyspace, "Example"}]}]}
]}.
